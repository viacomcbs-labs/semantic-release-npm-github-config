{
  "name": "@vc-vantage/semantic-release-npm-github-config",
  "version": "1.0.0",
  "description": "semantic-release shareable configuration for GitHub",
  "main": ".releaserc.json",
  "files": [
    ".releaserc.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vc-vantage/semantic-release-npm-github-config.git"
  },
  "keywords": [
    "semantic-release",
    "semantic-release-config",
    "github",
    "git",
    "release",
    "changelog"
  ],
  "author": "Piotr Plenik <piotr.plenik@vimn.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vc-vantage/semantic-release-npm-github-config/issues"
  },
  "homepage": "https://github.com/vc-vantage/semantic-release-npm-github-config#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "xo": {
    "prettier": true
  },
  "dependencies": {
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^5.5.8",
    "@semantic-release/npm": "^5.3.5",
    "@semantic-release/release-notes-generator": "^10.0.2"
  },
  "peerDependencies": {
    "semantic-release": ">=18.0.0"
  },
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "semantic-release": "^18.0.0",
    "xo": "^0.45.0"
  }
}
