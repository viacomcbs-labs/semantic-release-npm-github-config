[
	"@semantic-release/commit-analyzer",
	[
		"@semantic-release/release-notes-generator",
		{
			"presetConfig": {
				"issuePrefixes": ["VPM-"],
				"commitUrlFormat": "{{host}}/{{owner}}/{{repository}}/commits/{{hash}}",
				"compareUrlFormat": "{{host}}/{{owner}}/{{repository}}/compare/{{currentTag}}%0D{{previousTag}}#diff",
				"issueUrlFormat": "https://jira.mtvi.com/browse/{{prefix}}{{id}}"
			}
		}
	],
	"@semantic-release/github",
	[
		"@semantic-release/exec",
		{
				"verifyConditions": "echo 'a'",
				"publishCmd": "TAG=${nextRelease.version} docker buildx bake -f docker-bake.hcl"
		}
	]
]
